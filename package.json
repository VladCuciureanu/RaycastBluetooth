{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bluetooth",
  "title": "Bluetooth",
  "description": "Observe and control Bluetooth devices.",
  "icon": "logo.png",
  "author": "vladinski",
  "categories": [
    "Productivity",
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "manage-devices",
      "title": "Manage Devices",
      "subtitle": "Bluetooth",
      "description": "Opens a list of paired bluetooth devices.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2",
    "react": "^18.2.0",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "build": "ray build -e dist",
    "publish": "ray publish"
  }
}
